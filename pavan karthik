#include<stdio.h>
int a[3];
int main()
{ 
  int i,min,max;
  printf("enter the marks =");
  for(i=0;i<3;i++)
  {
  	scanf("%d",&a[i]);
  }
  min=a[0];
 max=a[0]; 
  for(i=1;i<3;i++)
  {
  	if(a[0]>a[i])
  	min=a[i];
  	if(a[0]<a[i])
  	max=a[i];
  }
  printf("minimum marks=%d\n",min);
  printf("maximum marks=%d\n",max);
  return 0;
}
